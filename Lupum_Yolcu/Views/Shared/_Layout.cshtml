        <section id="content_outer_wrapper">
            @RenderBody()
            <footer id="footer_wrapper">
                <div class="footer-content">
                    <div class="row">
                        <div class="col-xs-12 col-sm-6">
                            <h6>Want to Work with Us?</h6>
